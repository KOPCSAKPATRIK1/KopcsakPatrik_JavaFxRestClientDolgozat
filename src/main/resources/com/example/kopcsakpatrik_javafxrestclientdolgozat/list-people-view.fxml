<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox>
         <children>
            <Button fx:id="insertButton" mnemonicParsing="false" onAction="#insertClick" text="Insert">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin></Button>
            <Button fx:id="updateButton" mnemonicParsing="false" onAction="#updateClick" text="Update">
               <HBox.margin>
                  <Insets bottom="20.0" right="20.0" top="20.0" />
               </HBox.margin></Button>
            <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteClick" text="Delete">
               <HBox.margin>
                  <Insets bottom="20.0" right="20.0" top="20.0" />
               </HBox.margin></Button>
         </children>
      </HBox>
      <TableView fx:id="peopleTable" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="nameCol" minWidth="50.0" prefWidth="250.0" text="Name" />
          <TableColumn fx:id="salaryCol" minWidth="50.0" prefWidth="175.0" text="Salary" />
            <TableColumn fx:id="cvvCol" minWidth="50.0" prefWidth="100.0" text="CVV" />
        </columns>
      </TableView>
   </children>
</VBox>
